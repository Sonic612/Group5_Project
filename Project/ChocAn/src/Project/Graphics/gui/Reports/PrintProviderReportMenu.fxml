<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Graphics.Reports.PrintProviderReportController">
   <children>
      <TextField fx:id="provIDField" layoutX="107.0" layoutY="17.0" promptText="Enter Provider ID" />
      <Button layoutX="107.0" layoutY="101.0" mnemonicParsing="false" onAction="#onReportPrintClick" text="Print Report" />
      <Button layoutX="204.0" layoutY="101.0" mnemonicParsing="false" onAction="#onCancelClick" text="Cancel" />
      <Label layoutX="20.0" layoutY="21.0" text="Provider ID:" />
      <Label layoutX="20.0" layoutY="63.0" text="Date of Report:" />
      <DatePicker fx:id="datePickerField" editable="false" layoutX="107.0" layoutY="59.0" onAction="#onDateSelect" prefHeight="25.0" prefWidth="149.0" promptText="Select Report Date" />
      <Button fx:id="buttonDateReset" layoutX="14.0" layoutY="101.0" mnemonicParsing="false" onAction="#onDateResetClick" text="Reset Date" visible="false" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</Pane>
