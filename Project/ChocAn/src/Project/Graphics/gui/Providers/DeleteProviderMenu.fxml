<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Graphics.Providers.DeleteProviderController">
   <children>
      <Label layoutX="20.0" layoutY="19.0" text="Find Provider: " />
      <Label layoutX="20.0" layoutY="43.0" text="Provider ID: " />
      <TextField fx:id="provIDField" layoutX="85.0" layoutY="39.0" prefHeight="25.0" prefWidth="108.0" promptText="Enter Provider ID" />
      <Button layoutX="75.0" layoutY="78.0" mnemonicParsing="false" onAction="#onDeleteClick" text="Delete" />
      <Button layoutX="141.0" layoutY="78.0" mnemonicParsing="false" onAction="#onCancelClick" text="Cancel" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</Pane>
