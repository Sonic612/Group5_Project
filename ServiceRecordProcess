/**
 * Writes a Service Record to the database
 * @author jmilinsk
 *
 */
public class ServiceRecordProcess {
	
	private Connection dbConn;
	private String dbHost;
	private String dbUser;
	
/**
 * @param dbHost
 * @param dbUser
 */
public ServiceRecordProcess(String dbHost, String dbUser){
	this.dbHost = dbHost;
	this.dbUser = dbUser;
}

/**
 * @param serv
 * @return
 */
private String addService(Service serv){
	String added = "Service added";
	return added;
}

/**
 * @param serv
 * @return
 */
private String updService(Service serv){
	String updated = "Service updated";
	return updated;
}

/**
 * @param servCode
 * @return
 */
private String delService(int servCode){
	String deleted = "Service deleted";
	return deleted;
}

/**
 * 
 */
private void termConn(){
	
}

}
