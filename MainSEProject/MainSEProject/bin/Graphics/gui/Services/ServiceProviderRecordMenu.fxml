<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Graphics.Services.SPRecordController">
   <children>
      <Label layoutX="14.0" layoutY="18.0" text="Provider ID:" />
      <TextField fx:id="provIDField" layoutX="103.0" layoutY="15.0" prefHeight="25.0" prefWidth="157.0" promptText="Enter Provider ID" />
      <Label layoutX="14.0" layoutY="49.0" text="Member ID:" />
      <TextField fx:id="memIDField" layoutX="103.0" layoutY="46.0" prefHeight="25.0" prefWidth="157.0" promptText="Enter Member ID" />
      <Label layoutX="14.0" layoutY="80.0" text="Service Code: " />
      <TextField fx:id="servCodeField" layoutX="102.0" layoutY="77.0" prefHeight="25.0" prefWidth="157.0" promptText="Enter Service Code" />
      <Label layoutX="14.0" layoutY="110.0" text="Encounter Date: " />
      <DatePicker fx:id="encounterDatePickerField" editable="false" layoutX="102.0" layoutY="107.0" onAction="#onEncounterDateClick" prefHeight="25.0" prefWidth="157.0" promptText="Enter Encounter Date" />
      <Button fx:id="buttonDateReset" layoutX="18.0" layoutY="144.0" mnemonicParsing="false" onAction="#onDateResetClick" text="Reset Date" visible="false" />
      <Button layoutX="127.0" layoutY="144.0" mnemonicParsing="false" onAction="#onRecordClick" text="Record" />
      <Button layoutX="211.0" layoutY="144.0" mnemonicParsing="false" onAction="#onCancelClick" text="Cancel" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</Pane>
