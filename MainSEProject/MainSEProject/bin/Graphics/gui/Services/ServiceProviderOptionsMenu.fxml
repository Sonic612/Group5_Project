<?import javafx.scene.layout.*?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Graphics.Services.ServiceProviderOptionsMenuController">
   <children>
      <Label layoutX="183.0" layoutY="23.0" text="Service Provider Options:">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
      <Button layoutX="27.0" layoutY="62.0" mnemonicParsing="false" onAction="#onNewSerProvClick" prefHeight="52.0" prefWidth="102.0" text="Add Service Provider" wrapText="true" />
      <Button layoutX="143.0" layoutY="62.0" mnemonicParsing="false" onAction="#onDelSerProvClick" prefHeight="52.0" prefWidth="102.0" text="Delete Service Provider" wrapText="true" />
      <Button layoutX="262.0" layoutY="62.0" mnemonicParsing="false" onAction="#onUpdSerProvClick" prefHeight="52.0" prefWidth="102.0" text="Update Service Provider" wrapText="true" />
      <Button cancelButton="true" layoutX="194.0" layoutY="135.0" mnemonicParsing="false" onAction="#onBTMenuClick" text="Back to Main Menu" />
      <Button layoutX="379.0" layoutY="62.0" mnemonicParsing="false" onAction="#onSPRecordClick" prefHeight="52.0" prefWidth="102.0" text="Service Provider Record" wrapText="true" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</Pane>
